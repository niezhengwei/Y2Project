<!DOCTYPE web-app PUBLIC
 "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
 "http://java.sun.com/dtd/web-app_2_3.dtd" >

<web-app>
  <display-name>Archetype Created Web Application</display-name>
    <servlet>
        <servlet-name>newDaoServlet</servlet-name>
        <servlet-class>servlet.newDaoServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>newListServlet</servlet-name>
        <servlet-class>servlet.newListServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>newListServlet</servlet-name>
        <url-pattern>/servlet/newListServlet</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>newDaoServlet</servlet-name>
        <url-pattern>/servlet/newDaoServlet</url-pattern>
    </servlet-mapping>
</web-app>
